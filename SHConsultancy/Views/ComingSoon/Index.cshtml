@{ ViewBag.Title = "Coming Soon"; }



<!-- ==========================
    COMING SOON - START
=========================== -->
<section class="content-full bg-color-2">
    <div class="container comingsoon">
        <ul class="brands brands-md brands-inline">
            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
            <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
            <li><a href="#"><i class="fa fa-instagram"></i></a></li>
            <li><a href="#"><i class="fa fa-skype"></i></a></li>
        </ul>
        <div class="title">StoneHenge Consultancy</div>
        <h2>Our New Website is Coming Soon.</h2>
        <div class="countdown"></div>
    </div>
    <nav class="navbar navbar-comingsoon navbar-fixed-bottom bg-color-1">
        <div class="container">
            <p>Signup for Newsletter for stay in touch.</p>
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <form>
                        <fieldset>
                            <div class="form-group nospace">
                                <div class="input-group">
                                    <input type="email" class="form-control" id="email" placeholder="Put your email" required>
                                    <span class="input-group-btn"><button class="btn btn-primary" type="button">Submit</button></span>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </nav>
</section>
<!-- ==========================
COMING SOON - END
=========================== -->

@section scripts {
    <script src="http://code.jquery.com/jquery-latest.min.js"></script>
    <script src="http://code.jquery.com/ui/1.11.1/jquery-ui.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=true"></script>
}
