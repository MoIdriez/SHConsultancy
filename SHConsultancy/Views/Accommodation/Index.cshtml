@using SHConsultancy.Models
@{ ViewBag.Title = "Property"; }

<div id="page-wrapper">
    @Html.Partial("_Header", new HeaderViewModel { IsDefault = true, IsFixed = false })

    <!-- ==========================
        PORTFOLIO POST - START
    =========================== -->
    <section class="content bg-color-2" id="portfolio">
        <div class="container">
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1">
                    <div id="portfolio-slider" class="owl-carousel owl-theme">
                        <div class="item"><img src="~/images/portfolio/image_01.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_02.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_03.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_04.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_05.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_06.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_07.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_08.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_09.jpg" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/portfolio/image_10.jpg" alt="" class="img-responsive"></div>
                    </div>
                </div>
            </div>
            <!-- BLOG POST -->
            <div class="blog-post">
                <div class="blog-post-detail">
                    <span><i class="fa fa-calendar"></i>20/12/2016</span>
                </div>
                <div class="blog-post-body">
                    <a href="#"><h3>Properties</h3></a>
                    <p>Too find out more about the properties please contact us</p>
                    <div class="blog-post-buttons clearfix">
                        <div class="pull-left">
                            <a href="@Url.Action("Index", "ContactUs")" class="btn btn-primary">Contact Us</a>
                        </div>
                        <div class="pull-right">
                            <a href="#" class="btn btn-primary btn-xs"><i class="fa fa-twitter"></i>Tweet</a>
                            <a href="#" class="btn btn-primary btn-xs"><i class="fa fa-facebook"></i>Facebook</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>
    <!-- ==========================
        PORTFOLIO POST - END
    =========================== -->
    

    @Html.Partial("_Footer")
</div>