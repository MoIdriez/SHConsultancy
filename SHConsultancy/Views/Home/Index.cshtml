@using SHConsultancy.Models
@{ ViewBag.Title = "Home"; }

<div id="page-wrapper">

    @Html.Partial("_Header", new HeaderViewModel { IsDefault = true, IsFixed = true })

    <!-- ==========================
        JUMBOTRON - START
    =========================== -->
    <div class="jumbotron jumbotron6 jumbotron-full-height">
        <div id="jumbotron-slider" class="owl-carousel hidden-control owl-theme owl-animation">

            <div class="item" id="slide-2">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div><h2 class="animation top-to-bottom delay-1">PAGEHEADER1</h2></div>
                            <div><h3 class="animation bottom-to-top delay-2">PAGE. SUB. HEADER. 1</h3></div>
                            <div><h3 class="animation bottom-to-top delay-3">Page sub header 2</h3></div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="item" id="slide-1">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 col-md-offset-6">
                            <h2 class="animation right-to-left fast delay-1">PAGEHEADER2</h2>
                            <h3 class="animation right-to-left fast delay-2">Page sub header 3</h3>
                            <h3 class="animation right-to-left fast delay-3">Page sub header 4</h3>
                            <h3 class="animation right-to-left fast delay-4">Page sub header 5</h3>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- ==========================
        JUMBOTRON - END
    =========================== -->
    <!-- ==========================
        SERVICES - START
    =========================== -->
    <section class="content bg-color-2 services3" id="services">
        <div class="container">
            <h2 class="text-center">Our Services</h2>
            <div class="row">

                <!-- Service -->
                <div class="col-sm-4">
                    <div class="service">
                        <i class="fa fa-mobile"></i>
                        <div class="service-body">
                            <h3>Service1</h3>
                            <p>Service 1 body.</p>
                        </div>
                    </div>
                </div>

                <!-- Service -->
                <div class="col-sm-4">
                    <div class="service">
                        <i class="fa fa-pencil"></i>
                        <div class="service-body">
                            <h3>Service2</h3>
                            <p>Service 2 body.</p>
                        </div>
                    </div>
                </div>

                <!-- Service -->
                <div class="col-sm-4">
                    <div class="service">
                        <i class="fa fa-trophy"></i>
                        <div class="service-body">
                            <h3>Service3</h3>
                            <p>Service 3 body..</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========================
        SERVICES - END
    =========================== -->
    <!-- ==========================
        SEPARATOR - START
    =========================== -->
    <section class="content content-separator">
        <div class="container">
            <p class="text-lg text-center">Quote 1. - <b>Quote 1 Author</b></p>
        </div>
    </section>
    <!-- ==========================
        SEPARATOR - END
    =========================== -->
    <!-- ==========================
        ABOUT US - START
    =========================== -->
    <section class="content bg-color-2" id="section-about-us">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <h2>About Us</h2>
                    <p>About Us Paragraph 1.</p>
                    <p>About Us Paragraph 2.</p>
                    <div class="text-highlight">
                        <i class="fa fa-quote-left"></i>
                        <p>Recommendation text 1.</p>
                        <span>Recommendation Author 1</span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div id="about-us-slider" class="owl-carousel owl-theme">
                        <div class="item"><img src="~/images/showcase1.png" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/showcase2.png" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/showcase3.png" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/showcase4.png" alt="" class="img-responsive"></div>
                        <div class="item"><img src="~/images/showcase5.png" alt="" class="img-responsive"></div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========================
        ABOUT US - END
    =========================== -->
    <!-- ==========================
        STATISTICS - END
    =========================== -->
    <section class="content content-separator" id="section-statistics">
        <div class="container">
            <div class="row">
                <div class="col-xs-6 col-md-3">
                    <div class="statistic">
                        <div class="number">150</div>
                        <div class="title">Stat1</div>
                    </div>
                </div>
                <div class="col-xs-6 col-md-3">
                    <div class="statistic">
                        <div class="number">6</div>
                        <div class="title">Stat2</div>
                    </div>
                </div>
                <div class="col-xs-6 col-md-3">
                    <div class="statistic">
                        <div class="number">500</div>
                        <div class="title">Stat3</div>
                    </div>
                </div>
                <div class="col-xs-6 col-md-3">
                    <div class="statistic">
                        <div class="number">24</div>
                        <div class="title">Stat4</div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========================
    STATISTICS - END
    =========================== --> 
    <!-- ==========================
        PARTNERS - START
    =========================== -->
    <section class="content bg-color-2" id="section-portfolio">
        <div class="container">
            <h2>Partners</h2>
            <div class="row">
                <div class="col-md-4">
                    <article class="portfolio-item">
                        <img src="~/images/portfolio/image_01.jpg" class="img-responsive" alt="">
                        <div class="hover-overlay">
                            <p>Something about the partner.</p>
                            <a href="~/images/portfolio/image_01.jpg" class="show-image"><i class="fa fa-plus"></i></a>
                            <a href="#"><i class="fa fa-link"></i></a>
                        </div>
                    </article>
                    <div class="portfolio-item-description">
                        <h3>Partner 1</h3>
                    </div>
                </div>

                <div class="col-md-4">
                    <article class="portfolio-item">
                        <img src="~/images/portfolio/image_02.jpg" class="img-responsive" alt="">
                        <div class="hover-overlay">
                            <p>Something about the partner.</p>
                            <a href="~/images/portfolio/image_02.jpg" class="show-image"><i class="fa fa-plus"></i></a>
                            <a href="#"><i class="fa fa-link"></i></a>
                        </div>
                    </article>
                    <div class="portfolio-item-description">
                        <h3>Partner 2</h3>
                    </div>
                </div>

                <div class="col-md-4">
                    <article class="portfolio-item">
                        <img src="~/images/portfolio/image_03.jpg" class="img-responsive" alt="">
                        <div class="hover-overlay">
                            <p>Something about the partner.</p>
                            <a href="~/images/portfolio/image_03.jpg" class="show-image"><i class="fa fa-plus"></i></a>
                            <a href="#"><i class="fa fa-link"></i></a>
                        </div>
                    </article>
                    <div class="portfolio-item-description">
                        <h3>Partner 3</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========================
        PARTNERS - END
    =========================== -->
    <!-- ==========================
        CONTACT - START
    =========================== -->
    <section class="content" id="contact">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <h2>Leave a message</h2>
                    <div class="contact-alert-wrapper"></div>
                    <form id="contactForm">
                        <fieldset>

                            <div class="form-group control-group">
                                <div class="controls">
                                    <label for="name">Your name:<span>*</span></label>
                                    <p class="help-block"></p>
                                    <input type="text" class="form-control" placeholder="Your name:" id="name" data-validation-required-message="Please enter your name." aria-invalid="false">
                                </div>
                            </div>

                            <div class="form-group control-group">
                                <div class="controls">
                                    <label for="email2">Your email:<span>*</span></label>
                                    <p class="help-block"></p>
                                    <input type="email" class="form-control" placeholder="Your email:" id="email2" data-validation-required-message="Please enter your email address." data-validation-email-message="Not a valid email address." aria-invalid="false">
                                </div>
                            </div>

                            <div class="form-group control-group">
                                <div class="controls">
                                    <label for="message">Your message:<span>*</span></label>
                                    <p class="help-block"></p>
                                    <textarea class="form-control" placeholder="Your message:" id="message" data-validation-required-message="Please enter some message." aria-invalid="false"></textarea>
                                    <button type="submit" id="send" class="btn btn-primary btn-sm"><i class="fa fa-check"></i>Submit</button>
                                    <span class="form-info"><span class="required">*</span> These fields are required</span>
                                </div>
                            </div>

                        </fieldset>
                    </form>
                </div>
                <div class="col-sm-6">
                    <h2>Contact</h2>
                    <p>Contact Body 1.</p>
                    <ul class="list-unstyled contact-address">
                        <li>Contact Address (Company)</li>
                        <li><a>contact@email.com</a></li>
                        <li>contact phone number</li>
                    </ul>
                    <ul class="brands brands-md brands-inline brands-transition brands-circle main">
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========================
        CONTACT - END
    =========================== -->
    @Html.Partial("_Footer")

</div>
